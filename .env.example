# Venyu Flutter App Environment Configuration
# Copy this file to .env.local and fill in your actual values
# NEVER commit .env.local to version control

# Supabase Configuration - Development
SUPABASE_URL_DEV=https://your-dev-project.supabase.co
SUPABASE_KEY_DEV=your_development_anon_key_here

# Supabase Configuration - Staging
SUPABASE_URL_STAGING=https://your-staging-project.supabase.co
SUPABASE_KEY_STAGING=your_staging_anon_key_here

# Supabase Configuration - Production
SUPABASE_URL_PROD=https://your-production-project.supabase.co
SUPABASE_KEY_PROD=your_production_anon_key_here

# Google OAuth Configuration - Development
GOOGLE_IOS_CLIENT_ID_DEV=your_google_ios_client_id_dev
GOOGLE_WEB_CLIENT_ID_DEV=your_google_web_client_id_dev

# Google OAuth Configuration - Staging
GOOGLE_IOS_CLIENT_ID_STAGING=your_google_ios_client_id_staging
GOOGLE_WEB_CLIENT_ID_STAGING=your_google_web_client_id_staging

# Google OAuth Configuration - Production
GOOGLE_IOS_CLIENT_ID_PROD=your_google_ios_client_id_prod
GOOGLE_WEB_CLIENT_ID_PROD=your_google_web_client_id_prod

# Apple Sign-In Configuration - Development
# Service ID (registered in Apple Developer Console)
APPLE_CLIENT_ID_DEV=com.getvenyu.app.service
# Redirect URI (must match Supabase callback URL)
APPLE_REDIRECT_URI_DEV=https://your-dev-project.supabase.co/auth/v1/callback

# Apple Sign-In Configuration - Staging
APPLE_CLIENT_ID_STAGING=com.getvenyu.app.service
APPLE_REDIRECT_URI_STAGING=https://your-staging-project.supabase.co/auth/v1/callback

# Apple Sign-In Configuration - Production
APPLE_CLIENT_ID_PROD=com.getvenyu.app.service
APPLE_REDIRECT_URI_PROD=https://your-production-project.supabase.co/auth/v1/callback

# Environment (development, staging, production)
ENVIRONMENT=development