# Venyu Flutter App Environment Configuration
#
# IMPORTANT: This file is a template for environment configuration.
#
# Setup Instructions:
# 1. Copy this file to .env for local development
# 2. Fill in the actual values for your environment
# 3. NEVER commit .env to version control
#
# CI/CD Usage:
# In CI/CD pipelines, generate the .env file with environment-specific values.
#
# Example GitHub Actions:
#   - name: Create .env file
#     run: |
#       echo "SUPABASE_URL=${{ secrets.SUPABASE_URL_STAGING }}" >> .env
#       echo "SUPABASE_KEY=${{ secrets.SUPABASE_KEY_STAGING }}" >> .env
#       # ... add other secrets
#
# Example GitLab CI:
#   script:
#     - echo "SUPABASE_URL=${SUPABASE_URL_STAGING}" >> .env
#     - echo "SUPABASE_KEY=${SUPABASE_KEY_STAGING}" >> .env
#     # ... add other secrets

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_anon_key_here

# Google OAuth Configuration
GOOGLE_IOS_CLIENT_ID=your_google_ios_client_id
GOOGLE_WEB_CLIENT_ID=your_google_web_client_id

# Apple Sign-In Configuration
# Service ID (registered in Apple Developer Console)
APPLE_CLIENT_ID=com.getvenyu.app.service
# Redirect URI (must match Supabase callback URL)
APPLE_REDIRECT_URI=https://your-project.supabase.co/auth/v1/callback

# RevenueCat Configuration
REVENUECAT_APPLE_API_KEY=your_revenuecat_apple_api_key
REVENUECAT_GOOGLE_API_KEY=your_revenuecat_google_api_key
